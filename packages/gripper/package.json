    "chalk": "^4",
    "commander": "^12",
    "fastify": "^5.2.1",
    "graphql": "^16.10.0",
    "graphql-yoga": "^5.13.1",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2",
    "toad-scheduler": "^3.0.1",
    "typedi": "^0.10"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "esbuild": "^0.25",
    "eslint": "^8",
