{
  "name": "msgscan-indexer",
  "version": "0.1.0",
  "description": "msgscan indexer",
  "main": "build/index.js",
  "scripts": {
    "codegen": "npx graphql-codegen",
    "build": "shx rm -rf build && npm run codegen && tsc -b && shx cp -r graphql build",
