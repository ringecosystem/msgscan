{
  "name": "msgscan-indexer",
  "version": "0.1.0",
  "description": "msgscan indexer",
  "main": "build/index.js",
  "scripts": {
    "codegen": "npx graphql-codegen",
    "build": "shx rm -rf build && npm run codegen && tsc -b && shx cp -r graphql build",
    "package": "npm run codegen && node esbuild.js",
    "start": "node build/index.js",
    "lint": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "chalk": "^4",
    "commander": "^12",
    "graphql": "^16.10.0",
    "pino-pretty": "^13.0.0",
    "reflect-metadata": "^0.2",
    "typedi": "^0.10",
    "graphql-request": "^7.1.2",
    "@prisma/client": "6"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.5",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "esbuild": "^0.25",
    "eslint": "^8",
    "shx": "^0.3.4",
    "ts-node": "^10",
    "typescript": "^5",
    "prisma": "^6"
  },
  "files": [
    "dist",
    "build"
  ]
}
